{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "dub",
      "test": true,
      "buildType": "unittest",
      "configuration": "unittest",
      "problemMatcher": {
        "owner": "d",
        "fileLocation": "relative",
        "pattern": [
          {
            "regexp": "^.* thrown from (.*) on line (\\d+): (.*)$",
            "file": 1,
            "location": 2,
            "message": 3
          }
        ]
      },
      "group": {
        "kind": "test",
        "isDefault": true
      },
      "label": "dub test"
    },
    {
      "type": "dub",
      "run": false,
      "cwd": "${workspaceFolder}",
      "buildType": "$current",
      "configuration": "executable",
      "problemMatcher": [
        "$dmd"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "label": "dub build"
    }
  ]
}
